<%= link_to("New Member", {:action => :new}) %>
<table>
  <tr>
    <th> &nbsp; </th>
    <th>Code</th>
    <th>Name</th>
    <th>Type</th>
    <th>Action</th>
  </tr>
  <% @members.each do |member| %>
    <tr>
      <td><%= member.id %></td>
      <td><%= member.code %></td>
      <td><%= member.name %></td>
      <td><%= member.membership_type %></td>
      <td>
        <%= link_to("Show",   {:action => 'show',   :id => member.id}) %>
        <%= link_to("Edit",   {:action => 'edit',   :id => member.id}) %>
        <%= link_to("Delete", {:action => 'delete', :id => member.id}) %>
      </td>
    </tr>
  <% end %>
</table>
